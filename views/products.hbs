<div class="container">
    <h1>{{title}}</h1>
    <a href="/products/create-form" class="btn btn-success">Create Product</a>
    <br/> <br/>
    <table class="table">
        <tr>
            <th>#ID</th>
            <th>Product Name</th>
            <th>Website Name</th>
            <th>Price</th>
            <th>Review</th>
            <th>Rating</th>
            <th>Product Url</th>
            <th>Product_Img Url</th>
            <th>Action</th>
        </tr>
        {{#each products}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.product_name}}</td>
                <td>{{this.website_name}}</td>
                <td>{{this.price}}</td>
                <td>{{this.review}}</td>
                <td>{{this.rating}}</td>
                <td>{{this.Product_url}}</td>
                <td>{{this.product_image_url}}</td>
                <td>
                    <a href="/products/edit-form/{{this.id}}" class="btn btn-info btn-sm">Edit</a><br/><br/>
                    <a href="/products/delete/{{this.id}}" class="btn btn-danger btn-sm">Delete</a>
                </td>
            </tr>
        {{/each}}
    </table>
</div>




